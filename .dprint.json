{
    "$schema": "https://dprint.dev/schemas/v0.json",
    "indentWidth": 4,
    "lineWidth": 100,
    "newLineKind": "lf",
    "useTabs": false,
    "dockerfile": {},
    "exec": {
        "commands": [
            {
                "command": "rustfmt +nightly",
                "exts": ["rs"]
            }
        ]
    },
    "json": {},
    "jupyter": {},
    "malva": {
        "hexColorLength": "long",
        "declarationOrder": "smacss",
        "formatComments": "true",
        "trailingComma": "true"
    },
    "markdown": {
        "emphasisKind": "asterisks"
    },
    "markup": {
        "formatComments": "true",
        "scriptIndent": "true"
    },
    "ruff": {},
    "toml": {},
    "typescript": {},
    "yaml": {
        "indentWidth": 2,
        "bracketSpacing": true,
        "dashSpacing": "indent",
        "formatComments": true
    },
    "excludes": [
        "**/*-lock.json",
        "**/*.lock",
        "**/node_modules",
        "**/target"
    ],
    "plugins": [
        "https://plugins.dprint.dev/dockerfile-0.3.3.wasm",
        "https://plugins.dprint.dev/exec-0.6.0.json@a054130d458f124f9b5c91484833828950723a5af3f8ff2bd1523bd47b83b364",
        "https://plugins.dprint.dev/g-plane/malva-v0.15.0.wasm",
        "https://plugins.dprint.dev/g-plane/markup_fmt-v0.24.0.wasm",
        "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.1.wasm",
        "https://plugins.dprint.dev/json-0.21.0.wasm",
        "https://plugins.dprint.dev/jupyter-0.2.1.wasm",
        "https://plugins.dprint.dev/markdown-0.20.0.wasm",
        "https://plugins.dprint.dev/ruff-0.6.4.wasm",
        "https://plugins.dprint.dev/toml-0.7.0.wasm",
        "https://plugins.dprint.dev/typescript-0.95.11.wasm"
    ]
}
